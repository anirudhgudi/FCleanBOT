        <mesh filename="package://object/meshes/Obj.STL" scale="1 1 1" />
      </geometry>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1.0"/>
      </material>
    </visual>

    <!-- Collision -->
    <collision>
      <origin xyz="0 0 0" rpy="1.5704 0 0" />
      <geometry>
        <mesh filename="package://object/meshes/Obj.STL" scale="1 1 1" />
      </geometry>
    </collision>

